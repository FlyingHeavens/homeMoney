<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nodepower.homemoney.dao.FamilyMapper">
  <resultMap id="BaseResultMap" type="com.nodepower.homemoney.beans.Family">
    <id column="f_id" jdbcType="INTEGER" property="fId" />
    <result column="f_fname" jdbcType="VARCHAR" property="fFname" />
    <result column="f_pname" jdbcType="VARCHAR" property="fPname" />
    <result column="f_role" jdbcType="VARCHAR" property="fRole" />
    <result column="f_age" jdbcType="INTEGER" property="fAge" />
    <result column="f_p_id" jdbcType="INTEGER" property="fPId" />
  </resultMap>

  <update id="updateByExample">
    update bill where b_id=#{bid}
  </update>
</mapper>